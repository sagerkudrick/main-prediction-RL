<?xml version="1.0" ?>
<robot name="custom_object">
  <link name="body">
    <!-- Link frame: Z axis points up through the top of the box -->
    <visual>
      <!-- No rotation, origin at center -->
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="models/isotopebox.obj" scale="0.1 0.1 0.1"/>
      </geometry>
    </visual>

    <collision>
      <!-- Same as visual -->
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="models/isotopebox.obj" scale="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <inertial>
      <!-- Centered and aligned with link frame -->
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.05"/>
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
    </inertial>
  </link>
</robot>
